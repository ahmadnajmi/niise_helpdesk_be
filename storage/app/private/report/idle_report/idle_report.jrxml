	<dataset name="Total Insiden" uuid="78edd639-f6d9-4327-80cc-c3c67e08ebae">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Oracle Localhost"/>
		<parameter name="branch_id" class="java.lang.String"/>
		<parameter name="state_id" class="java.lang.String"/>
		<parameter name="severity_id" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{severity_id}]]></defaultValueExpression>
		</parameter>
		<parameter name="status" class="java.lang.String"/>
		<parameter name="company_id" class="java.lang.String"/>
		<parameter name="start_date" class="java.lang.String"/>
		<parameter name="end_date" class="java.lang.String"/>
		<parameter name="close_end_date" class="java.lang.String"/>
		<parameter name="close_start_date" class="java.lang.String"/>
		<query language="SQL"><![CDATA[SELECT 
    ROW_NUMBER() OVER (ORDER BY b.name, c.name) AS No,
    b.name AS Cawangan,
    c.name AS Category,
    COUNT(i.id) AS Total_Insiden
FROM incidents i
JOIN branch b ON i.branch_id = b.id
JOIN categories c ON i.category_id = c.id
LEFT JOIN (
    SELECT 
        incident_id,
        MAX(updated_at) AS latest_updated_at
    FROM incident_resolution
    GROUP BY incident_id
) ir ON ir.incident_id = i.id
LEFT JOIN sla_version sv ON sv.id = i.sla_version_id
LEFT JOIN sla_template st ON st.id = sv.sla_template_id
LEFT JOIN companies cp ON cp.id = i.assign_group_id
WHERE i.status in (1,2,5,6)
AND CAST(NVL(ir.latest_updated_at, i.incident_date) AS DATE) < SYSDATE - 4
AND ($P{branch_id} IS NULL OR i.branch_id = $P{branch_id})
AND ($P{severity_id} IS NULL OR st.SEVERITY_ID = $P{severity_id})
AND ($P{status} IS NULL OR i.status = $P{status})
AND ($P{state_id} IS NULL OR b.state_id = $P{state_id})
AND ($P{company_id} IS NULL OR cp.id = $P{company_id})
AND ( $P{start_date} IS NULL OR i.incident_date >= TO_DATE($P{start_date}, 'YYYY-MM-DD'))
AND ($P{end_date} IS NULL OR i.incident_date < TO_DATE($P{end_date}, 'YYYY-MM-DD') + 1)
AND ( $P{close_start_date} IS NULL OR i.actual_end_date >= TO_DATE($P{close_start_date}, 'YYYY-MM-DD'))
AND ($P{close_end_date} IS NULL OR i.actual_end_date < TO_DATE($P{close_end_date}, 'YYYY-MM-DD') + 1)
GROUP BY b.name, c.name
HAVING COUNT(i.id) > 0
ORDER BY b.name, c.name]]></query>
