	<parameter name="close_end_date" class="java.lang.String"/>
	<query language="sql"><![CDATA[	SELECT i.id AS incident_id,incident_no,b.name AS branch_name,cp.name AS contractor_name,cs.nickname AS complaint_name,
	cs.phone_no AS complaint_phone_no,incident_date,actual_end_date,ir.latest_updated_at AS last_end_date,
	st.severity_id,cy.name AS category_name,information,rt.name AS status
	FROM incidents i
	LEFT JOIN branch b ON b.id = i.branch_id
	LEFT JOIN users cs ON cs.id = i.complaint_user_id
	LEFT JOIN sla_version sv ON sv.id = i.sla_version_id
	LEFT JOIN sla_template st ON st.id = sv.sla_template_id
	JOIN categories cy ON cy.id = i.category_id
	LEFT JOIN users csc ON csc.id = i.resolved_user_id
    LEFT JOIN companies cp ON cp.id = csc.company_id
	LEFT JOIN ref_table rt ON rt.ref_code = i.status AND rt.code_category = 'incident_status'
	LEFT JOIN (
	    SELECT 
	        incident_id,
	        MAX(updated_at) AS latest_updated_at
	    FROM incident_resolution
	    GROUP BY incident_id
	) ir ON ir.incident_id = i.id
	WHERE i.status in (1,2,5,6)
	AND CAST(NVL(ir.latest_updated_at, i.incident_date) AS DATE) < SYSDATE - 4
	AND ($P{branch_id} IS NULL OR i.branch_id = $P{branch_id})
	AND ($P{severity_id} IS NULL OR st.SEVERITY_ID = $P{severity_id})
	AND ($P{status} IS NULL OR i.status = $P{status})
	AND ($P{state_id} IS NULL OR b.state_id = $P{state_id})
	AND ($P{company_id} IS NULL OR cp.id = $P{company_id})
	AND ( $P{start_date} IS NULL OR i.incident_date >= TO_DATE($P{start_date}, 'YYYY-MM-DD'))
	AND ($P{end_date} IS NULL OR i.incident_date < TO_DATE($P{end_date}, 'YYYY-MM-DD') + 1)
    AND ( $P{close_start_date} IS NULL OR i.actual_end_date >= TO_DATE($P{close_start_date}, 'YYYY-MM-DD'))
    AND ($P{close_end_date} IS NULL OR i.actual_end_date < TO_DATE($P{close_end_date}, 'YYYY-MM-DD') + 1)
	ORDER BY i.incident_no DESC]]></query>
